{
	"name": "vscode-frez",
	"displayName": "vscode-frez",
	"publisher": "frez",
	"description": "",
	"repository": "https://github.com/jacktrades/vscode-frez",
	"version": "0.0.40",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-frez.log",
				"title": "Frez - Log"
			},
			{
				"command": "vscode-frez.moveUpChunk",
				"title": "Frez - Move Up Chunk"
			},
			{
				"command": "vscode-frez.moveDownChunk",
				"title": "Frez - Move Down Chunk"
			},
			{
				"command": "vscode-frez.moveUpChunks",
				"title": "Frez - Move Up Chunks"
			},
			{
				"command": "vscode-frez.moveDownChunks",
				"title": "Frez - Move Down Chunks"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack --config ./build/node-extension.webpack.config.js",
		"watch": "webpack --watch --config ./build/node-extension.webpack.config.js",
		"package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.52.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.15.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1",
		"ts-loader": "^8.0.11",
		"webpack": "^5.10.0",
		"webpack-cli": "^4.2.0"
	},
	"dependencies": {
		"jsonc-parser": "3.0.0"
	},
	"#extensionDependencies": []
}
